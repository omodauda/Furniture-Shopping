{
    "presets": ["module:metro-react-native-babel-preset"],
    "plugins": [
      ["module-resolver", {
        "root": ["./src"],
        "alias": {
          "@assets": "./assets",
          "@components": "./src/components",
          "@constants": "./src/constants",
          "@screens": "./src/screens",
          "@navigations": "./src/navigations",
          "@validations": "./src/validations",
          "@data": "./src/data",
          "@store": "./src/store"
        }
      }],
      ["module:react-native-dotenv", {
        "envName": "APP_ENV",
        "moduleName": "@env",
        "path": ".env",
        "allowUndefined": false
      }]
    ]
}